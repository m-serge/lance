{
  "name": "lance-gg",
  "version": "1.0.9",
  "description": "A Node.js based real-time multiplayer game server",
  "keywords": [
    "pixijs",
    "pixi",
    "socket",
    "canvas",
    "WebGL",
    "HTML5",
    "AFrame",
    "physics",
    "engine",
    "game",
    "realtime",
    "multiplayer"
  ],
  "dependencies": {
    "cannon": "^0.6.2",
    "eventemitter3": "^1.2.0",
    "mkdirp": "^0.5.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  },
  "files": [
    "dist"
  ],
  "author": "Opher Vishnia",
  "contributors": [
    {
      "name": "Opher Vishnia"
    },
    {
      "name": "Gary Weiss"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lance-gg/lance/issues"
  },
  "homepage": "https://github.com/lance-gg/lance#readme",
  "scripts": {
